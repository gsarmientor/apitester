					"script": {
						"id": "19861c8a-1f0f-4d13-9380-346718d37906",
						"exec": [
							"var ids = pm.environment.get(\"newId\");\r",
							"var currentId = ids.shift();\r",
							"\r",
							"pm.environment.set(\"idFromArray\", currentId);\r",
							"pm.environment.set(\"newId\", ids);\r",
							""
						],
						"type": "text/javascript"
